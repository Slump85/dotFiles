{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "java",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "java.jdt.ls.vmargs": "-javaagent:/usr/local/share/java/lombok.jar:/usr/share/idea/lib/idea_rt.jar=45051:/usr/share/idea/bin -Dfile.encoding=UTF-8",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "path": "/usr/lib/jvm/java-8-openjdk"
    },
    {
      "name": "JavaSE-11",
      "path": "/usr/lib/jvm/java-11-openjdk"
    },
    {
      "name": "JavaSE-17",
      "path": "/usr/lib/jvm/java-17-openjdk",
      "default": true
    }
  ],
  "java.format.settings.url": "/home/slump/.config/nvim/formatters/Default-intelliJ.xml",
  "java.referencesCodeLens.enabled": true,
  "java.implementationsCodeLens.enabled": true,
  "java.maven.downloadSources": true,
  "java.import.maven.enabled": true,
  "java.completion.enabled": true,
  "java.format.enabled": true,
  "java.codeGeneration.toString.codeStyle": "STRING_CONCATENATION",
  "java.codeGeneration.toString.template": "${object.className} [${member.name()}=${member.value}, ${otherMembers}]",
  "java.saveActions.organizeImports": true,
  "java.format.comments.enabled": true,
  "java.completion.importOrder": [
      "java",
      "javax",
      "com",
      "org"
  ],
  "java.completion.maxResults": 10,
  "java.completion.filteredTypes": [
      "java.awt.*",
      "com.sun.*"
  ],
  "codeLens.enable": true,
  "java.configuration.maven.userSettings": "$HOME/.m2/settings.xml"
}

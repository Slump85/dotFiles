
! =================
!       Xft
! =================
Xft.dpi:96
Xft.antialias:true
Xft.rgba:subpixel
Xft.hinting:true
Xft.hintstyle:hintslight
Xft.lcdfilter:lcddefault
*font: xft:JetBrains Mono:style=Regular:size=16
*boldFont: xft:JetBrains Mono:style=Bold:size=16
*italicFont: xft:JetBrains Mono:style=Italic:size=16
*boldItalicfont: xft:JetBrains Mono:style=Bold Italic:size=16

! ===================
!       urxvt
! ===================
! URxvt.perl-ext-common:resize-font,default,matcher,url-select
! URxvt.url-select.launcher: /usr/bin/xdg-open
! URxvt.url-select.underline:true
! URxvt.matcher.button: 1
! URxvt*scrollBar:false
! URxvt*scrollBar_right:false
! URxvt*letterSpace:0
! URxvt*lineSpace:0
! URxvt.internalBorder:50
! URxvt.geometry: 92x24
! URxvt*cursorBlink:true
! URxvt.iso14755:false
! URxvt*utf8:true
! URxvt.depth: 32
! URxvt.background:[70]#000000

URxvt.keysym.C-equal:resize-font:bigger
URxvt.keysym.C-minus:resize-font:smaller
URxvt.keysym.Shift-Control-C:eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V:eval:paste_clipboard

